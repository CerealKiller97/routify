{
    "name": "@roxi/routify",
    "version": "3.0.0-next.22",
    "main": "lib/runtime/index.js",
    "type": "module",
    "bin": {
        "routify3": "./lib/cli/index.js"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": "./lib/runtime/index.js",
        "./vite-plugin": "./lib/extra/vite-plugin.js",
        "./lib/extra/vite-plugin": "./lib/extra/vite-plugin.js",
        "./lib/*": "./lib/*.js",
        "./lib/cmp/*": "./lib/cmp/*.js",
        "./typings/*": "./typings/*",
        "./plugins/*": "./plugins/*"
    },
    "scripts": {
        "build": "run-s build:*",
        "build:lint": "npm run util:prettier -- --write",
        "build:documentation": "documentation readme typedef.js -f md --section API",
        "build:typings": "tsc ",
        "test": "run-s test:*",
        "test:jest": "jest . --detectOpenHandles",
        "test:lint": "npm run util:prettier -- --check",
        "util:prettier": "prettier -c --plugin-search-dir=. \"{common,lib,plugins,test}/**/*.{js,svelte}\" --ignore-path .gitignore",
        "semantic-release": "semantic-release"
    },
    "author": "jakobrosenberg@gmail.com",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^18.0.0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@testing-library/svelte": "^3.0.3",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.0.1",
        "@types/prompts": "^2.0.14",
        "documentation": "^13.2.5",
        "fkill": "^7.2.1",
        "jest": "^27.0.6",
        "jest-playwright-preset": "^1.6.1",
        "npm-run-all": "^4.1.5",
        "playwright": "^1.10.0",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.2.0",
        "rollup": "^2.47.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-svelte": "^7.1.0",
        "semantic-release": "^17.4.7",
        "spassr": "^2.6.0",
        "svelte-jester": "^2.1.3",
        "tree-kill": "^1.2.2",
        "typescript": "^4.5.0-beta"
    },
    "dependencies": {
        "cachewrap": "^0.0.1",
        "cheap-watch": "^1.0.3",
        "cheerio": "^0.x",
        "commander": "^7.2.0",
        "configent": "^3.0.0-0",
        "consolite": "^0.2.9",
        "fs-extra": "^9.1.0",
        "hookar": "^0.0.1",
        "kleur": "^4.1.4",
        "persistable": "^0.1.2",
        "prompts": "^2.4.1",
        "svelte": "^3.39.0"
    },
    "prettier": {
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "all",
        "bracketSpacing": true,
        "arrowParens": "avoid",
        "semi": false,
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 90,
        "svelteSortOrder": "options-scripts-markup-styles",
        "svelteAllowShorthand": true,
        "svelteBracketNewLine": false,
        "svelteIndentScriptAndStyle": true
    },
    "workspaces": [
        "examples/*",
        "examples/microframework/*"
    ]
}
